{
  "name": "n8n-nodes-wax",
  "version": "1.0.0",
  "description": "n8n community node for WAX blockchain - NFTs, AtomicAssets, AtomicMarket, gaming, and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "wax",
    "blockchain",
    "nft",
    "atomicassets",
    "atomicmarket",
    "eosio",
    "antelope",
    "web3",
    "crypto",
    "gaming"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://velobpa.com",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-wax.git"
  },
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-wax/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WaxNetwork.credentials.js",
      "dist/credentials/AtomicAssets.credentials.js",
      "dist/credentials/WaxCloudWallet.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Wax/Wax.node.js",
      "dist/nodes/Wax/WaxTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "eosjs": "^22.1.0",
    "axios": "^1.6.2",
    "node-fetch": "^2.7.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
